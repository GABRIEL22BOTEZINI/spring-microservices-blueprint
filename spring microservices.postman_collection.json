		{
			"name": "Sign in",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"200\", () => pm.response.to.have.status(200));",
							"",
							"let body = {};",
							"try { body = pm.response.json(); } catch (e) {}",
							"",
							"const token = body.token || body.accessToken || \"\";",
							"const type = body.type || \"Bearer\";",
							"",
							"pm.globals.set(\"token\", token);",
							"pm.globals.set(\"authHeader\", `${type} ${token}`);",
							"pm.globals.set(\"jwt\", token); // optional mirror",
							"",
							"// Avoid shadowing: clear same vars in env/collection if present",
							"pm.environment.unset(\"token\");",
							"pm.collectionVariables.unset(\"token\");",
							"",
							"pm.test(\"Token captured\", () => pm.expect(token).to.not.be.empty);"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
